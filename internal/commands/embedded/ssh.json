      "tags": ["socks", "proxy", "dynamic"]
    },
    {
      "template": "ssh -J JUMP_HOST USER@TARGET_HOST",
      "description": "Connect via jump host (bastion)",
      "tags": ["jump", "bastion", "proxy"]
    },
    {
      "template": "ssh -o StrictHostKeyChecking=no USER@HOST",
      "description": "Connect without host key verification",
      "tags": ["nocheck", "insecure", "first"]
    },
    {
      "template": "ssh -v USER@HOST",
      "description": "Verbose connection for debugging",
      "tags": ["debug", "verbose", "troubleshoot"]
    },
    {
      "template": "ssh -N -f -L LOCAL_PORT:REMOTE_HOST:REMOTE_PORT USER@HOST",
      "description": "Background tunnel without shell",
      "tags": ["background", "tunnel", "daemon"]
    },
    {
      "template": "ssh-keygen -t rsa -b 4096 -C \"EMAIL\"",
      "description": "Generate new RSA SSH key pair",
      "tags": ["keygen", "generate", "rsa"]
    },
    {
      "template": "ssh-keygen -t ed25519 -C \"EMAIL\"",
      "description": "Generate new Ed25519 SSH key (recommended)",
      "tags": ["keygen", "generate", "ed25519"]
    },
    {
      "template": "ssh-copy-id USER@HOST",
      "description": "Copy public key to remote host",
      "tags": ["copy", "key", "authorize"]
    },
    {
      "template": "ssh-add ~/.ssh/id_rsa",
      "description": "Add private key to SSH agent",
      "tags": ["agent", "add", "key"]
    },
    {
      "template": "scp FILE USER@HOST:REMOTE_PATH",
