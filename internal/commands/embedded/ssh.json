{
  "category": "ssh",
  "commands": [
    {
      "template": "ssh USER@HOST",
      "description": "Connect to remote host via SSH",
      "tags": ["connect", "remote", "login"]
    },
    {
      "template": "ssh -p PORT USER@HOST",
      "description": "Connect to remote host on specific port",
      "tags": ["connect", "port", "remote"]
    },
    {
      "template": "ssh -i KEYFILE USER@HOST",
